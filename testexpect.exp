#!/usr/bin/expect

log_user 0

spawn ls
expect {
	default { send_user "Error: Cannot find \'libft\'\n" ; exit 1 }
	"libft"
}
send_user "\'libft\' was found.\n"
close

spawn ls -l
expect {
	default { send_user "Error: Cannot find \'libft\'\n" ; exit 1 }
	"libft"
}
send_user "\'libft\' was found.\n"
close
